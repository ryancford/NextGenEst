<?xml version="1.0" encoding="UTF-8" ?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
	<!--<item android:state_activated="false" android:state_pressed="false" android:state_selected="false" android:drawable="@co"/> normal -->


	<item android:state_activated="true"><!-- activated -->
		<shape
			android:shape="ring"
			android:innerRadiusRatio="2.13"
			android:thicknessRatio="50"
			android:useLevel="false">
			<solid android:color="@color/white"/>
		</shape>
	</item>
	<item android:state_selected="true"><!-- selected -->
		<shape
			android:shape="ring"
			android:innerRadiusRatio="2.13"
			android:thicknessRatio="50"
			android:useLevel="false">
			<solid android:color="@color/white"/>
		</shape>
	</item>

	<item android:state_pressed="true"><!-- pressed -->
	<shape
		android:shape="ring"
		android:innerRadiusRatio="2.13"
		android:thicknessRatio="50"
		android:useLevel="false">
		<solid android:color="@color/white"/>
	</shape>
</item>
</selector>
